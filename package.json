{
  "name": "angulario-number-input",
  "description": "Material styled number input for Angular",
  "version": "0.4.1",
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 127.0.0.1",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --single-run --no-progress --browsers HeadlessChrome",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gulp": "gulp inline-templates",
    "build-lib": "gulp inline-templates && ngc -p tsconfig-lib.json",
    "preversion": "git pull -r",
    "postversion": "git push && git push --tags",
    "publish-lib": "npm run build-lib && npm publish",
    "deploy": "ng build --prod --base-href \"/angulario-number-input/\"  && ngh",
    "build:prod": "ng build --prod"
  },
  "precommit": [
    "lint",
    "test:ci",
    "build:prod"
  ],
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Dyskovsky/angulario-number-input"
  },
  "keywords": [
    "Angular",
    "input",
    "number",
    "material"
  ],
  "private": false,
  "dependencies": {},
  "devDependencies": {
    "@angular/cli": "1.6.3",
    "@angular/common": "5.1.3",
    "@angular/compiler": "5.1.3",
    "@angular/compiler-cli": "5.2.1",
    "@angular/core": "5.1.3",
    "@angular/forms": "5.1.3",
    "@angular/language-service": "5.1.3",
    "@angular/platform-browser": "5.1.3",
    "@angular/platform-browser-dynamic": "5.1.3",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^11.10.0",
    "angular-cli-ghpages": "^0.5.2",
    "codelyzer": "^4.0.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-inline-ng2-template": "^4.0.0",
    "jasmine-core": "~2.9.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "pre-commit": "^1.2.2",
    "protractor": "~5.3.0",
    "puppeteer": "^0.13.0",
    "ts-node": "~4.1.0",
    "tslint": "5.9.1",
    "typescript": "~2.6.2"
  },
  "peerDependencies": {
    "@angular/common": ">4.0.0",
    "@angular/core": ">4.0.0",
    "@angular/forms": ">4.0.0"
  }
}
